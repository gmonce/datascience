        "# Generate values for X,Y,Z, given the parameters\n",
        "def simulate(r,p1,p2,p3,p4,q1,q2):\n",
        "\n",
        "    # Pick a value for Z (Syndrome): 1 (presence), 0 (absence)\n",
        "    z=np.random.choice([0,1],p=[1-r,r])\n",
        "\n",
        "    # Pick a value for X (Treatment): 1 (take), 0 (don't take)\n",
        "    if (z==0):\n",
        "        x=np.random.choice([0,1],p=[1-q1,q1])\n",
        "    else:\n",
        "        x=np.random.choice([0,1],p=[1-q2,q2])\n",
        "\n",
        "    # Pick a value for Y (Outcome): 1 (death), 0 (survive)\n",
        "    if (z==0 and x==1):\n",
        "        y=np.random.choice([0,1],p=[1-p2,p2])\n",
        "    elif (z==0 and x==0):\n",
        "        y=np.random.choice([0,1],p=[1-p1,p1])\n",
        "    elif (z==1 and x==0):\n",
        "        y=np.random.choice([0,1],p=[1-p3,p3])\n",
        "    else:\n",
        "        y=np.random.choice([0,1],p=[1-p4,p4])\n",
        "\n",
        "    # Return the assigned values\n",
        "    return (x,y,z)\n",
        "\n",
        "# % of the population that has the syndrome\n",
        "r=1/2\n",
        "\n",
        "# Probability of dying when taking the drug, if the patient does not have the syndrome\n",
        "p2 = 1/3\n",
        "\n",
        "# Probability of dying when not taking the drug, if the patient does not have the syndrome\n",
        "p1 = 2/5\n",
        "\n",
        "# Probability of dying when taking the drug, if the patient does  have the syndrome\n",
        "p4 = 1/4\n",
        "\n",
        "# Probability of dying when not taking the drug, if the patient does  have the syndrome\n",
        "p3 = 1/2\n",
